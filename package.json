{
  "name": "app-flowershop",
  "version": "1.0.0",
  "description": "Front-end for Flowershop sample application",
  "main": "lib/app/index",
  "typings": "lib/app/index",
  "scripts": {
    "build": "node_modules/.bin/tsc -p src/app --outDir lib/app",
    "build:w": "node_modules/.bin/tsc -w -p src/app --outDir lib/app",
    "build-tests": "node_modules/.bin/tsc -p src/test --outDir lib",
    "build-tests:w": "node_modules/.bin/tsc -p -w src/test --outDir lib",
    "pretest": "npm run build-tests",
    "test": "node_modules/.bin/mocha lib/test/**/*.spec.js -c",
    "prestart": "npm run build",
    "start": "node ."
  },
  "keywords": [],
  "dependencies": {
    "compoxure": "1.0.15",
    "cookie-parser": "1.4.3",
    "corpjs-amqp": "0.0.6",
    "corpjs-config": "0.0.11",
    "corpjs-endpoints": "0.0.13",
    "corpjs-express": "0.0.9",
    "corpjs-logger": "0.0.13",
    "corpjs-system": "0.0.36",
    "express": "4.14.0",
    "mkdirp": "0.5.1",
    "morgan": "1.7.0",
    "parxer": "1.1.3"
  },
  "devDependencies": {
    "@types/mocha": "2.2.39",
    "@types/node": "7.0.5",
    "mocha": "3.2.0",
    "typescript": "2.2.1"
  }
}
