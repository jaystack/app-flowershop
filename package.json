{
  "name": "app-flowershop",
  "version": "1.0.0",
  "description": "Front-end for Flowershop sample application",
  "main": "lib/app/index",
  "typings": "lib/app/index",
  "scripts": {
    "build": "node_modules/.bin/tsc -p src/app --outDir lib/app",
    "build:w": "node_modules/.bin/tsc -w -p src/app --outDir lib/app",
    "build-tests": "node_modules/.bin/tsc -p src/test --outDir lib",
    "build-tests:w": "node_modules/.bin/tsc -p -w src/test --outDir lib",
    "pretest": "npm run build-tests",
    "test": "node_modules/.bin/mocha lib/test/**/*.spec.js -c",
    "prestart": "npm run build",
    "start": "node ."
  },
  "keywords": [],
  "dependencies": {
    "compoxure": "1.0.15",
    "cookie-parser": "^1.4.3",
    "corpjs-amqp": "latest",
    "corpjs-config": "latest",
    "corpjs-endpoints": "latest",
    "corpjs-express": "latest",
    "corpjs-logger": "latest",
    "corpjs-system": "latest",
    "express": "^4.14.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.7.0",
    "parxer": "1.1.3"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "mocha": "^3.2.0",
    "typescript": "^2.2.1"
  },
  "_dependencies_": {
    "config": "^1.21.0",
    "system-endpoints": "0.0.3",
    "winston": "^2.3.1"
  },
  "_devDependencies_": {
    "@types/config": "0.0.30",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.33",
    "@types/express-serve-static-core": "^4.0.39",
    "@types/mime": "0.0.29",
    "@types/mocha": "^2.2.32",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.46",
    "@types/serve-static": "^1.7.31",
    "@types/compoxure": "file:./@types/compoxure",
    "mocha": "^3.1.2",
    "mocha-junit-reporter": "^1.12.1",
    "typescript": "^2.2.1"
  }
}
